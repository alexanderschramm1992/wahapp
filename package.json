{
  "name": "wahapp",
  "version": "0.0.1",
  "description": "Quick tag-based 40k library",
  "main": "elm.js",
  "scripts": {
    "prestart": "rimraf ./dist",
    "start": "parcel build src/index.html --public-url ./ && copyfiles -V -f ./src/img/* ./dist/img",
    "poststart": "live-server ./dist",
    "dev": "watch \"elm make src/Main.elm --output=app.js\" src"
  },
  "keywords": [
    "40k",
    "tags",
    "elm"
  ],
  "author": "Alexander Schramm",
  "license": "MIT",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "cordova": "^11.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "elm": "^0.19.1-5",
    "elm-hot": "^1.1.6",
    "elm-live": "^2.7.5",
    "node-elm-compiler": "^5.0.6",
    "watch": "^1.0.2"
  }
}
